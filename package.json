{
  "name": "e2e-terminal-automation",
  "version": "1.0.0",
  "description": "Simple end‑to‑end terminal automation with constant answer engine",
  "main": "dist/stage/main/index.js",
  "bin": {
    "sistem": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc && tsc -p tsconfig.admin.json && cp src/native/admin/package.json dist/admin/",
    "start": "node dist/cli.js",
    "test": "node e2e/verify.js",
    "test:electron": "playwright test",
    "checkmate": "ts-node scripts/checkmate.ts",
    "dev:stage": "electron-vite dev",
    "build:stage": "electron-vite build && mkdir -p dist/stage/native && cp -r src/native/pkg dist/stage/native/",
    "preview:stage": "electron-vite preview",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint",
    "ecy:start": "python src/ecy/cli.py start",
    "ecy:run": "python src/ecy/cli.py run",
    "ecy:self-improve": "python src/ecy/cli.py self-improve"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@axe-core/playwright": "4.11.0",
    "@electron-toolkit/preload": "3.0.2",
    "@electron-toolkit/utils": "4.0.0",
    "@playwright/test": "1.44.0",
    "@types/chroma-js": "^3.1.2",
    "@types/ink-big-text": "1.2.4",
    "@types/ink-gradient": "2.0.4",
    "@types/ink-spinner": "3.0.5",
    "@types/ink-text-input": "2.0.5",
    "@types/node": "22.12.0",
    "@types/react-dom": "19.2.3",
    "@types/winston": "2.4.4",
    "@vitejs/plugin-react": "5.1.2",
    "axe-core": "4.11.1",
    "electron": "33.2.1",
    "electron-vite": "5.0.0",
    "gts": "7.0.0",
    "knip": "5.80.2",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "rollup-plugin-visualizer": "6.0.5",
    "ts-node": "10.9.2",
    "typescript": "5.6.3",
    "typescript-eslint": "8.53.0",
    "vite": "7.3.1"
  },
  "build": {
    "appId": "com.antigravity.stage",
    "mac": {
      "entitlements": "build/entitlements.mac.plist",
      "extendInfo": {
        "NSAppleEventsUsageDescription": "Antigravity needs to control System Events to execute your automation tasks."
      }
    }
  },
  "dependencies": {
    "@lancedb/lancedb": "0.23.0",
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.5.0",
    "@tensorflow/tfjs": "4.22.0",
    "@tensorflow/tfjs-backend-webgpu": "4.22.0",
    "axios": "1.13.2",
    "chroma-js": "^3.2.0",
    "clsx": "^2.1.1",
    "dotenv": "17.2.3",
    "electron-log": "5.4.3",
    "electron-store": "11.0.2",
    "framer-motion": "^12.29.2",
    "ink": "6.6.0",
    "ink-big-text": "2.0.0",
    "ink-gradient": "3.0.0",
    "ink-spinner": "5.0.0",
    "ink-text-input": "6.0.0",
    "lucide-react": "^0.563.0",
    "matter-js": "^0.20.0",
    "msgpackr": "1.11.8",
    "ollama": "0.6.3",
    "react-markdown": "^10.1.0",
    "react-reconciler": "0.33.0",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^3.4.0",
    "three": "^0.182.0",
    "winston": "3.19.0",
    "yoga-layout-prebuilt": "1.10.0"
  }
}
